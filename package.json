{
	"devDependencies": {
		"@11ty/eleventy": "^1.0.2",
		"@11ty/eleventy-navigation": "^0.3.5",
		"prettier": "^2.7.1"
	},
	"scripts": {
		"build": "npx eleventy",
		"clean": "rm -rf _distribution",
		"format:distribution": "npx prettier --write _distribution",
		"postbuild": "npm run format:distribution",
		"start": "npm run clean && npx eleventy --serve"
	}
}
